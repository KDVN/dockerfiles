<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
        <record id="seq_type_kderp_prepaid_purchase_code" model="ir.sequence.type">
            <field name="name">KDERP Prepaid Purchase Order Code</field>
            <field name="code">kderp_prepaid_order_code</field>
        </record>        
        
        <record id="seq_kderp_prepaid_purchase_order_code_hanoi" model="ir.sequence">
            <field name="name">KDERP Prepaid Purchase Order HANOI</field>
            <field name="code">kderp_prepaid_order_code</field>
            <field eval="4" name="padding"/>
            <field name="prefix">HPPO</field>
            <field name="suffix">YY-</field>
            <field name="example">HPPOYY-0000</field>
            <field name="padding">4</field>
        </record>
        
        <record id="seq_kderp_prepaid_purchase_order_code_hcm" model="ir.sequence">
            <field name="name">KDERP Prepaid Purchase Order HCM</field>
            <field name="code">kderp_prepaid_order_code</field>
            <field eval="4" name="padding"/>
            <field name="prefix">SPPO</field>
            <field name="suffix">YY-</field>
            <field name="example">SPPOYY-0000</field>
            <field name="padding">4</field>
        </record>        
        
    </data>
</openerp>