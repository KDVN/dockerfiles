		<menuitem id="menu_kderp_budget_job_budget" parent="kderp_base.menu_kderp_budget_control_root" name="Job &amp; Budget" sequence="15"/>
		
		<record id="view_kderp_analytic_tree_inherit" model="ir.ui.view">
			<field name="name">view.analytic.tree.kderp.total.budget.inherit</field>
			<field name="model">account.analytic.account</field>
			<field name="inherit_id" ref="account.view_account_analytic_account_tree"/>
			<field name="arch" type="xml">
				<field name='name' position='replace'>
					<field name='name'/>
				</field>
			</field>
		</record>
		
		<record id="view_kderp_analytic_form_inherit" model="ir.ui.view">
