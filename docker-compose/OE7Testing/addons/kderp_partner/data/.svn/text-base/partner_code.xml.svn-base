<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        
        <!--
	Sequences types for KDERP Project
	-->
        <record id="seq_type_kderp_partner_supplier_code" model="ir.sequence.type">
            <field name="name">KDERP Supplier Code</field>
            <field name="code">kderp_supplier_code</field>
        </record>
       
<!--         <record id="seq_type_kderp_partner_supplier_code_han" model="ir.sequence.type">
            <field name="name">KDERP Supplier Code</field>
            <field name="code">kderp_supplier_code_han</field>
        </record> -->
         
        <record id="seq_type_kderp_partner_customer_code" model="ir.sequence.type">
            <field name="name">KDERP Supplier Code</field>
            <field name="code">kderp_customer_code</field>
        </record>
        
	<!--
	Sequences for KDERP Supplier & Customer
	-->
		<!--  Ha Noi -->
		<!-- Supplier Procurement -->
        <record id="seq_kderp_hn_supplier_code" model="ir.sequence">
            <field name="name">Supplier Code in Hanoi area</field>
            <field name="code">kderp_supplier_code</field>
            <field eval="4" name="padding"/>
            <field name="prefix">P0</field>
            <field name="suffix">0000</field>
        </record>
        
        <!-- Supplier Vietnam Not VAT -->
        <record id="seq_kderp_hn_supplier_not_vat_code" model="ir.sequence">
            <field name="name">Supplier Code in Hanoi Area, Vietnam Not VAT Invoice</field>
            <field name="code">kderp_supplier_code</field>
            <field eval="4" name="padding"/>
            <field name="prefix">P1</field>
            <field name="suffix">0000</field>
        </record>
        
        <!-- Supplier International -->
        <record id="seq_kderp_hn_supplier_internation_code" model="ir.sequence">
            <field name="name">Supplier Code in Hanoi area, Internation</field>
            <field name="code">kderp_supplier_code</field>
            <field eval="4" name="padding"/>
            <field name="prefix">P2</field>
            <field name="suffix">0000</field>
        </record>
        
        <record id="seq_kderp_hcm_supplier_code" model="ir.sequence">
            <field name="name">Supplier Code in HCM area</field>
            <field name="code">kderp_supplier_code</field>
            <field eval="4" name="padding"/>
            <field name="prefix">PS</field>
            <field name="suffix">0000</field>
        </record>

        <record id="seq_kderp_hn_customer_code" model="ir.sequence">
            <field name="name">Customer Code in Hanoi area</field>
            <field name="code">kderp_customer_code</field>
            <field eval="4" name="padding"/>
            <field name="prefix">C</field>
            <field name="suffix">0000</field>
        </record>

        <record id="seq_kderp_hn_other_customer_code" model="ir.sequence">
            <field name="name">Other Customer Code in Hanoi area</field>
            <field name="code">kderp_customer_code</field>
            <field eval="3" name="padding"/>
            <field name="prefix">C9</field>
            <field name="suffix">000</field>
        </record>
        
        <record id="seq_kderp_hcm_customer_code" model="ir.sequence">
            <field name="name">Customer Code in HCM area</field>
            <field name="code">kderp_customer_code</field>
            <field eval="4" name="padding"/>
            <field name="prefix">CS</field>
            <field name="suffix">0000</field>
        </record>
        
    </data>
</openerp>