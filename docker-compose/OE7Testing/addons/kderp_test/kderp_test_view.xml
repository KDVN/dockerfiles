			</field>
        </record> 
        
		<record id="action_view_kderp_test_view" model="ir.actions.act_window">
			<field name="name">Example Python</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">kderp.test</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>
        <menuitem 
        			action="action_view_kderp_test_view" 
        			id="kderp_menu_kderp_test_action" 
        			parent="kderp_test_menu" sequence="20"/>
