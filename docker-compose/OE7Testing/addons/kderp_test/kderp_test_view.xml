        	id="kderp_test_menu" name="Example" sequence="5" parent="kderp_test_root"/>
        <record id="view_kderp_test_view" model="ir.ui.view">
            <field name="name">kderp.test.form</field>
            <field name="model">kderp.test</field>
            <field name="type">form</field>
	      <field name="arch" type="xml">
				<form string="Test">
					<field name="list_code" string="Code"/>
