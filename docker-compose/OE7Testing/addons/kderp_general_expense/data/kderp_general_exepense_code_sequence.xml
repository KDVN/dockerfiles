<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data >
        <!-- Sequences for kderp.hanoi.general.expense.code --> 
        <record id="seq_type_kderp_general_expense_code" model="ir.sequence.type">
            <field name="name">KDERP General Expense Code</field>
            <field name="code">kderp.general.expense.code</field>
        </record>
        <record id="seq_type_kderp_supplier_payment_general_expense_code" model="ir.sequence.type">
            <field name="name">KDERP Supplier Payment General Expense Code</field>
            <field name="code">kderp.supplier.payment.ge.code</field>
        </record>
        <record id="seq_kderp_general_expense_code" model="ir.sequence">
            <field name="name">KDERP Hanoi General Expense Code</field>
            <field name="code">kderp.general.expense.code</field>
            <field name="prefix">HGE%(year)s</field>
            <field name="padding">0</field>
            <field name="number_next_actual">0</field>
            <field name="number_increment">0</field>
			<field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
    
        <record id="seq_kderp_hcm_general_expense_code" model="ir.sequence">
            <field name="name">KDERP HCM General Expense Code</field>
            <field name="code">kderp.general.expense.code</field>
            <field name="prefix">SGE%(year)s</field>
            <field name="padding">0</field>
            <field name="number_next_actual">0</field>
            <field name="number_increment">0</field>
			<field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
         <record id="seq_kderp_supplier_payment_general_expense_code" model="ir.sequence">
            <field name="name">KDERP Supplier Payment General Expense Code</field>
            <field name="code">kderp.supplier.payment.ge.code</field>
            <field name="suffix">YY-</field>
            <field name="prefix">GN</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_next_actual">0</field>
            <field name="number_increment">1</field>
			<field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
