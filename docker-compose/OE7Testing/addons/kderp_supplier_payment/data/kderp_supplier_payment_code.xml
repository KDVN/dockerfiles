<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        
        <!--
	Sequences types for KDERP Client Payment
	-->
        <record id="seq_type_kderp_supplier_payment_code" model="ir.sequence.type">
            <field name="name">KDERP Supplier Payment Code</field>
            <field name="code">kderp_supplier_payment_code</field>
        </record>
        
         <record id="seq_type_kderp_supplier_payment_code_ot" model="ir.sequence.type">
            <field name="name">KDERP Supplier Payment Code</field>
            <field name="code">kderp_supplier_payment_code_ot</field>
        </record>
        
	<!--
	Sequences for KDERP Client Payment
	-->
        <record id="seq_kderp_supplier_payment_code" model="ir.sequence">
            <field name="name">KDERP Supplier Payment</field>
            <field name="code">kderp_supplier_payment_code</field>
            <field eval="4" name="padding"/>
            <field name="prefix">IN</field>
            <field name="suffix">YY-</field>
            <field name="example">IN11-0000</field>
            <field name="padding">4</field>
        </record>
        
       <record id="seq_kderp_supplier_payment_ot_code" model="ir.sequence">
            <field name="name">KDERP Supplier Payment Other Expense</field>
            <field name="code">kderp_supplier_payment_code_ot</field>
            <field eval="4" name="padding"/>
            <field name="prefix">EN</field>
            <field name="suffix">YY-</field>
            <field name="example">EN11-0000</field>
            <field name="padding">4</field>
        </record>
        
        
        
    </data>
</openerp>